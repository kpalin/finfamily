<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>suku11</display-name>
	<servlet>
		<description>
		</description>
		<display-name>SukuServlet</display-name>
		<servlet-name>SukuServlet</servlet-name>
		<servlet-class>
		fi.kaila.suku.server.SukuServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SukuServlet</servlet-name>
		<url-pattern>/SukuServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<env-entry>
		<description>jdbc driver </description>
		<env-entry-name>suku.db.driver</env-entry-name> 
		<env-entry-type>java.lang.String</env-entry-type>	 
		<env-entry-value>org.postgresql.Driver</env-entry-value>	
  	</env-entry>
    
  <env-entry>
	<description>server address</description>
	<env-entry-name>suku.db.server</env-entry-name> 
	<env-entry-type>java.lang.String</env-entry-type>	 
	<env-entry-value>localhost</env-entry-value>
  </env-entry>
    
  <env-entry>
	<description>server database</description>
	<env-entry-name>suku.db.database</env-entry-name> 
	<env-entry-type>java.lang.String</env-entry-type>	 
	<env-entry-value>suku11</env-entry-value>
  </env-entry>

    
  <env-entry>
	<description>database user</description>
	<env-entry-name>suku.db.user</env-entry-name> 
	<env-entry-type>java.lang.String</env-entry-type>	 
	<env-entry-value>kalle</env-entry-value>
  </env-entry>

    
  <env-entry>
	<description>database password</description>
	<env-entry-name>suku.db.password</env-entry-name> 
	<env-entry-type>java.lang.String</env-entry-type>	 
	<env-entry-value>kalle</env-entry-value>
  </env-entry>
  
    
  <env-entry>
	<description>upload folder</description>
	<env-entry-name>suku.upload.folder</env-entry-name> 
	<env-entry-type>java.lang.String</env-entry-type>	 
	<env-entry-value>C:/Apps/upload/</env-entry-value>
  </env-entry>
</web-app>
